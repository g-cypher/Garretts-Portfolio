      <script>
          const scriptURL = 'https://script.google.com/macros/s/AKfycbwTY6qmvqgBW-S9E2ph74W0ruams0k49710bfBAsfHmktbA4GF9/exec'
          const form = document.forms['test-form']
        
          form.addEventListener('submit', e => {
            fetch(scriptURL, { method: 'POST', body: new FormData(form)})
