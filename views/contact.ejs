      <section id="cover">
        <div id="cover-caption">
          <div id="loginContainer" class="container">
            <div class="row">
              <div class="col-sm-6 offset-sm-3">
                <form>
                  <div class="form-group">
                    <h1 class="text-center font-italic contact-label">CONTACT</h1>
                  </div>
                  <div class="form-group">
                    <input id="firstName" class="form-control" name="firstName" type="text" placeholder="First Name">
                  </div>
                  <div class="form-group">
                    <input id="lastName" class="form-control" name="lastName" type="text" placeholder="Last Name">
                  </div>
                  <div class="form-group">
                    <input id="email" class="form-control" name="email" type="text" placeholder="Email">
                  </div>
                    <button class="btn btn-block bg-dark text-white contact-btn"type="submit" id="submit-form">Submit</button>
                </form>
              </div>
            </div>
          </div>
        </div>
      </section>   
